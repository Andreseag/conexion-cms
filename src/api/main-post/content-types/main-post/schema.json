{
  "kind": "collectionType",
  "collectionName": "main_posts",
  "info": {
    "singularName": "main-post",
    "pluralName": "main-posts",
    "displayName": "Main Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "author": {
      "type": "string"
    },
    "body": {
      "type": "richtext"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "discharges": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "main_posts"
    }
  }
}
